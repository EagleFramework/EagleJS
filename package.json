{
  "name": "@eagleirons/eaglejs",
  "description": "EagleJS is a jQuery-Like DOM manipulation class for modern browsers",
  "version": "0.5.4",
  "author": "Cem Demirkartal",
  "license": "MIT",
  "homepage": "https://github.com/eagleirons/eaglejs",
  "keywords": [
    "javascript",
    "jquery",
    "es6",
    "module",
    "dom-manipulation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eagleirons/eaglejs.git"
  },
  "bugs": {
    "url": "https://github.com/eagleirons/eaglejs/issues"
  },
  "directories": {
    "doc": "docs"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "main": "dist/eaglejs.js",
  "module": "dist/eaglejs.esm.js",
  "types": "dist/eaglejs.esm.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "docdash": "^1.2.0",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-markdown": "^2.0.0-rc.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jsdoc": "^3.6.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "terser": "^5.5.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "npm run build-esm && npm run eslint-fix-types && npm run build-script && npm run build-esm-min && npm run build-script-min && npm run build-docs",
    "build-docs": "npm run delete-docs && npm run create-docs",
    "build-esm": "npm run tsc-build && npm run eslint-fix-esm",
    "build-esm-min": "terser dist/eaglejs.esm.js --compress --mangle --output dist/eaglejs.esm.min.js",
    "build-script": "npm run rollup-build && npm run eslint-fix-script",
    "build-script-min": "terser dist/eaglejs.js --compress --mangle --output dist/eaglejs.min.js",
    "create-docs": "jsdoc -c .jsdoc.json",
    "delete-docs": "rimraf docs/",
    "eslint-fix-esm": "eslint --fix dist/eaglejs.esm.js",
    "eslint-fix-script": "eslint --fix dist/eaglejs.js",
    "eslint-fix-types": "eslint --fix dist/eaglejs.esm.d.ts",
    "rollup-build": "rollup src/eaglejs.js -o dist/eaglejs.js",
    "tsc-build": "tsc --project tsconfig.json"
  }
}
