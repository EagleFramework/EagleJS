{
  "name": "@eagleirons/eaglejs",
  "description": "EagleJS is a jQuery-Like DOM manipulation library for modern browsers",
  "version": "0.6.2",
  "author": "Cem Demirkartal",
  "license": "MIT",
  "homepage": "https://github.com/eagleirons/eaglejs",
  "keywords": [
    "javascript",
    "jquery",
    "es6",
    "module",
    "dom-manipulation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eagleirons/eaglejs.git"
  },
  "bugs": {
    "url": "https://github.com/eagleirons/eaglejs/issues"
  },
  "directories": {
    "doc": "docs"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "main": "dist/eaglejs.js",
  "module": "dist/eaglejs.esm.js",
  "types": "dist/eaglejs.esm.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "docdash": "^1.2.0",
    "eslint": "^7.17.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.11",
    "eslint-plugin-markdown": "^2.0.0-rc.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jsdoc": "^3.6.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "terser": "^5.5.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:script && npm run lint:fix-types && npm run build:docs",
    "build:docs": "npm run docs:delete && npm run docs:create",
    "build:esm": "npm run tsc:esm && npm run lint:fix-esm && npm run minify:esm",
    "build:script": "npm run rollup:script && npm run lint:fix-script && npm run minify:script",
    "docs:create": "jsdoc --configure .jsdoc.json",
    "docs:delete": "rimraf docs/",
    "lint": "eslint --cache src/eaglejs.esm.ts",
    "lint:fix": "eslint --fix src/eaglejs.esm.ts",
    "lint:fix-esm": "eslint --fix dist/eaglejs.esm.js",
    "lint:fix-script": "eslint --fix dist/eaglejs.js",
    "lint:fix-types": "eslint --fix dist/eaglejs.esm.d.ts",
    "minify:esm": "terser dist/eaglejs.esm.js --compress --mangle --output dist/eaglejs.esm.min.js",
    "minify:script": "terser dist/eaglejs.js --compress --mangle --output dist/eaglejs.min.js",
    "rollup:script": "rollup src/eaglejs.js -o dist/eaglejs.js",
    "tsc:esm": "tsc --project tsconfig.json"
  }
}
