{
  "name": "eaglejs",
  "description": "EagleJS is a jQuery-Like DOM manipulation class for modern browsers",
  "version": "0.5.0",
  "license": "MIT",
  "main": "./eaglejs.js",
  "module": "./eaglejs.esm.js",
  "author": "Cem Demirkartal",
  "repository": {
    "type": "git",
    "url": "https://github.com/EagleFramework/EagleJS"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "docdash": "^1.2.0",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "jsdoc": "^3.6.6",
    "terser": "^5.3.8",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "build": "npm run build-esm && npm run build-esm-min && npm run create-docs",
    "build-esm": "npm run tsc-build && npm run eslint-fix-esm",
    "build-esm-min": "terser ./eaglejs.esm.js --compress --mangle --output ./eaglejs.esm.min.js",
    "build-main-min": "terser ./eaglejs.js --compress --mangle --output ./eaglejs.min.js",
    "create-docs": "jsdoc -c ./.jsdoc.json",
    "eslint-fix-esm": "eslint --fix ./eaglejs.esm.js",
    "eslint-fix-main": "eslint --fix ./eaglejs.js",
    "tsc-build": "tsc --project ./tsconfig.json"
  }
}
