{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./README.md", "./dist/eaglejs.esm.js"]
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "template": "./node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs/"
  },
  "docdash": {
    "sort": true,
    "search": true,
    "collapse": true,
    "sectionOrder": ["Modules", "Classes"],
    "menu": {
      "Github repo": {
        "href": "https://github.com/eagleirons/eaglejs",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      }
    }
  }
}
